		"sources": {
			"contracts/interfaces/IOrderManager.sol": {
				"content": "// SPDX-License-Identifier: MIT\r\npragma solidity ^0.8.0;\r\nimport \"../structs/OrderStructs.sol\";\r\ninterface IOrderManager {\r\n    event OrderCreated(uint orderId, address user, uint totalAmount, OrderStatus status, OrderItem[] orderItems);\r\n    event OrderUpdated(uint orderId, OrderStatus status);\r\n    event OrderDeleted(uint orderId);\r\n\r\n    function createOrder(\r\n        address _user,\r\n        string memory _userInfo,\r\n        OrderItemRequest[] memory _items\r\n    ) external returns (uint);\r\n\r\n    // function getOrder(uint _orderId) external view returns (Order memory);\r\n\r\n    // function updateOrder(\r\n    //     uint _orderId,\r\n    //     string memory _userInfo,\r\n    //     OrderItem[] memory _items\r\n    // ) external;\r\n\r\n    // function deleteOrder(uint _orderId) external;\r\n\r\n    // function getOrdersByStatus(OrderStatus _status)\r\n    //     external\r\n    //     view\r\n    //     returns (Order[] memory);\r\n\r\n    // function updateOrderStatus(uint _orderId, OrderStatus _status) external;\r\n    // function getUserOrders(address _user) external view returns (uint[] memory);\r\n}"
			},
			"contracts/structs/OrderStructs.sol": {
				"content": "// SPDX-License-Identifier: MIT\r\npragma solidity ^0.8.0;\r\n\r\nenum OrderStatus { Pending, Confirmed, Cancelled }\r\n\r\nstruct OrderItem {\r\n    uint orderItemId;\r\n    uint name;\r\n    uint quantity;\r\n    uint price;\r\n    string imageUrl;\r\n}\r\n\r\nstruct OrderItemRequest {\r\n    uint name;\r\n    uint quantity;\r\n    uint price;\r\n    string imageUrl;\r\n}\r\n\r\nstruct Order {\r\n    uint orderId;\r\n    address user;\r\n    string userInfo;\r\n    OrderItem[] items;\r\n    uint totalAmount;\r\n    OrderStatus status;\r\n    uint timestamp;\r\n}\r\n"
			}
		},
